<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity WoWorkOrder.
    -->
    <changeSet id="20190521150338-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="from_location_id"
                                 baseTableName="wo_work_order"
                                 constraintName="fk_wo_work_order_from_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location_type"/>

        <addForeignKeyConstraint baseColumnNames="to_location_id"
                                 baseTableName="wo_work_order"
                                 constraintName="fk_wo_work_order_to_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location_type"/>

        <addForeignKeyConstraint baseColumnNames="wo_sales_agent_id"
                                 baseTableName="wo_work_order"
                                 constraintName="fk_wo_work_order_wo_sales_agent_id"
                                 referencedColumnNames="id"
                                 referencedTableName="wo_sales_agent"/>

        <addForeignKeyConstraint baseColumnNames="insurance_type_id"
                                 baseTableName="wo_work_order"
                                 constraintName="fk_wo_work_order_insurance_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="insurance_type"/>

    </changeSet>
</databaseChangeLog>
